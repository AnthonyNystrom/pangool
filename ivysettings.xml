<ivysettings>

  <properties file="../conf/project.properties"/>

  <settings defaultResolver='chain-resolver' />
        
  <property name="snapshot.upload.url" value="http://nexus.viewsflow.com/nexus/content/repositories/snapshots"/>
  <property name="release.upload.url" value="http://nexus.viewsflow.com/nexus/content/repositories/releases"/>
  <credentials host="nexus.viewsflow.com" realm="Sonatype Nexus Repository Manager" username="admin" passwd="Fiatibkag1"/>
    
  <resolvers>
  
    <ibiblio name='public' root='http://nexus.viewsflow.com/nexus/content/groups/public/' checkmodified="true" m2compatible='true' checkconsistency="false" />
    <ibiblio name='thirdparty' root='http://nexus.viewsflow.com/nexus/content/repositories/thirdparty/' m2compatible='true' checkconsistency="false"/>
    <ibiblio name='apache' root='http://nexus.viewsflow.com/nexus/content/repositories/apache-snapshots/' m2compatible='true' checkconsistency="false"/>
    <ibiblio name='central' root='http://nexus.viewsflow.com/nexus/content/shadows/central-m1/' m2compatible='true' checkconsistency="false"/>
    <ibiblio name='mirror1' root='http://nexus.viewsflow.com/nexus/content/repositories/mirror1/' m2compatible='true' checkconsistency="false"/>
    <ibiblio name='codehaus' root='http://nexus.viewsflow.com/nexus/content/repositories/codehaus-snapshots/' m2compatible='true' checkconsistency="false"/>
    <ibiblio name='google' root='http://nexus.viewsflow.com/nexus/content/repositories/google/' m2compatible='true' checkconsistency="false"/>
    <ibiblio name='javanet' root='http://nexus.viewsflow.com/nexus/content/repositories/java.net-m2/' m2compatible='true' checkconsistency="false"/>    
    <ibiblio name='nexus-central' root='http://nexus.viewsflow.com/nexus/content/repositories/central/' m2compatible='true' checkconsistency="false"/>
    <ibiblio name='alfresco' root='http://nexus.viewsflow.com/nexus/content/repositories/alfresco' m2compatible='true' checkconsistency="false"/>
 	<ibiblio name='mromll' root='http://mromll.googlecode.com/svn/repo' m2compatible='true' checkconsistency="false"/>
 	<ibiblio name='boilerpipe' root='http://boilerpipe.googlecode.com/svn/repo' m2compatible='true' checkconsistency="false"/>
 <!-- Cloudera repo -->

    <ibiblio name="cloudera" m2compatible="true" 
        pattern="[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" 
        root="http://nexus.viewsflow.com/nexus/content/repositories/cloudera/" checkconsistency="false"/>
                	
 <!-- Upload releases repo -->
 
  	<url name="nexus-release" m2compatible="true">
       <artifact pattern="${release.upload.url}/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]" />
   	   <ivy pattern="${release.upload.url}/[organisation]/[module]/[revision]/ivy-[revision].xml" />
    </url>
 	<url name="nexus-snapshot" checkmodified="true" m2compatible="true">
       <artifact pattern="${snapshot.upload.url}/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]" />
   	   <ivy pattern="${snapshot.upload.url}/[organisation]/[module]/[revision]/ivy-[revision].xml" />
    </url>
        
    <chain name="chain-resolver" checkmodified="true" returnFirst="true">
     	<resolver ref='nexus-snapshot'/>
     	<resolver ref='nexus-release'/>
     	<resolver ref='public'/>
     	<resolver ref='thirdparty'/>
     	<resolver ref='apache'/>
     	<resolver ref='central'/>
     	<resolver ref='codehaus'/>
     	<resolver ref='google'/>
     	<resolver ref='javanet'/>
     	<resolver ref='cloudera'/>
     	<resolver ref='mirror1'/>
     	<resolver ref='nexus-central'/>
     	<resolver ref='alfresco'/>
     	<resolver ref='mromll'/>
     	<resolver ref='boilerpipe'/>
    </chain>
  </resolvers>
</ivysettings>